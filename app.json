{
  "expo": {
    "name": "Geez To Amharic",
    "slug": "geez-to-amharic",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.7.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splashImage/d37cc919-bbbf-4b92-ad24-edb68147035d-46.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.geettoamharicTranslation"
    },
    "description": "Geez to Amharic machine translation developed ",
    "plugins": [
      [
        "expo-font",
        [
          "expo-camera",
          {
            "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
            "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
            "recordAudioAndroid": true
          }
        ]
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ]
    ],
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.geettoamharicTranslation"
    },
    "extra": {
      "eas": {
        "projectId": "15b06924-236e-43c3-ba2b-c6063166c793"
      }
    }
  }
}
